
using System;
using System.Collections.Generic;
using System.Linq;

class Program
{

	// Function to print the diagonals
	static int diagonal(int A)
	{
        int count = 0;
        string result = "";
        while (A > 0)
        {
            
            int value = A % 2;
            A = A / 2;
            if (result == "1")
                count++;

        }


        return count;
    }

	// Driver code
	public static void Main(string[] args)
	{

		// Matrix initialisation
		diagonal(11);
	}
}


