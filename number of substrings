
using System;
using System.Collections.Generic;

class GFG
{

    public static int palindromecheck(string A)
    {
        int count = 0;
        for (int i = 0; i < A.Length; i++)
        {
            if (A[i] == 'a' || A[i] == 'e' || A[i] == 'i' || A[i] == 'o' || A[i] == 'u' || A[i] == 'A' || A[i] == 'E' || A[i] == 'I' || A[i] == 'O' || A[i] == 'U')
            {
                count += A.Length - i;

            }
        }

        return count;

    }
  
    public static void Main()
	{
        string A = "";     
        palindromecheck(A);
    }
}

